{
  "name": "your-vendor/filament-multi-side-select",
  "description": "A multi-side select component with Ajax search for Filament v3",
  "keywords": [
    "laravel",
    "filament",
    "select",
    "multi-select",
    "ajax"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "filament/filament": "^3.0",
    "illuminate/support": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "YourVendor\\FilamentMultiSideSelect\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourVendor\\FilamentMultiSideSelect\\FilamentMultiSideSelectServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}